class palindrome:
    def palindromeornot(self,n):
        m=n
        r=0
        while n>0:
            dig=n%10
            r=r*10+dig
            n=n//10
        if m==n:
            print('Given number is palindrome')
        else:
            print('Given number is not palindrome')
n=int(input())
a=palindrome()
a.palindromeornot(n)
