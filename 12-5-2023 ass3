class Solution:
    # Complete this function
    
    #Function to find equilibrium point in the array.
    def equilibriumPoint(self,A, N):
        rsum=sum(A)
        lsum=0
        for i in range(0,N):
            rsum=rsum-A[i]
            if lsum==rsum:
                return i+1
            lsum=lsum+A[i]
        return -1
